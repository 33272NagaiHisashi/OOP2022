            <CheckBox Content="宮崎" x:Name="cbMiyasaki" Grid.Row="1" Grid.Column="5" VerticalAlignment="Center"/>
            <CheckBox Content="鹿児島" x:Name="cbKagoshima" Grid.Row="1" Grid.Column="6" VerticalAlignment="Center"/>
            <CheckBox Content="沖縄" x:Name="cbOkinawa" Grid.Row="1" Grid.Column="7" VerticalAlignment="Center"/>
        </Grid>
        <Grid Grid.Row="9">
            <Button Content="Button" Name="tbAcquisition" Click="tbAcquisition_Click"/>
        </Grid>
    </Grid>
</Window>
