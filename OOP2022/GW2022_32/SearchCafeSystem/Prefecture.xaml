            <CheckBox Content="大阪" x:Name="cbOsaka" Grid.Row="1" Grid.Column="3" VerticalAlignment="Center"/>
            <CheckBox Content="奈良" x:Name="cbNara" Grid.Row="1" Grid.Column="4" VerticalAlignment="Center"/>
            <CheckBox Content="和歌山" x:Name="cbWakayama" Grid.Row="1" Grid.Column="5" VerticalAlignment="Center"/>
