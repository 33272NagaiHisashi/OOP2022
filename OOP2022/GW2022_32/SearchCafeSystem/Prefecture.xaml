            <CheckBox Content="岩手" x:Name="cbIwate" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center"/>
            <CheckBox Content="秋田" x:Name="cbAkita" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center"/>
