        <Button Content="店名検索" HorizontalAlignment="Left" Margin="90,135,0,0" VerticalAlignment="Top" Width="75" Height="45"/>
        <Button Content="都道府県" x:Name="btPrefecture" HorizontalAlignment="Left" Margin="90,235,0,0" VerticalAlignment="Top" Width="75" Height="45" Click="btPrefecture_Click"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="200,25,0,0" VerticalAlignment="Top" Height="50" Width="380" Background="#FFF0A7A7"/>
        <TextBox HorizontalAlignment="Left" Height="45" Margin="205,135,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="370"/>

    </Grid>
</Window>
